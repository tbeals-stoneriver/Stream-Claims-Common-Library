<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<xs:include schemaLocation="../xsd/PolicyImport.xsd" />
	<xs:include schemaLocation="../xsd/ClaimCommon.xsd" />
	<xs:include schemaLocation="../xsd/CodeLookupCommon.xsd" />
	<xs:include schemaLocation="../xsd/ClaimUnit.xsd" />
	<xs:include schemaLocation="../xsd/Assignment.xsd" />
	<xs:include schemaLocation="../xsd/Case.xsd" />
	<xs:include schemaLocation="../xsd/WorkItem.xsd" />
	<xs:include schemaLocation="../xsd/ClaimBill.xsd" />
	<xs:include schemaLocation="../xsd/ClaimPayment.xsd" />
	<xs:include schemaLocation="../xsd/Document.xsd" />
	<xs:include schemaLocation="../xsd/ClaimRecovery.xsd" />
	<xs:include schemaLocation="../xsd/ClaimReserve.xsd" />
	<xs:include schemaLocation="../xsd/FinancialView.xsd" />
	<xs:include schemaLocation="../xsd/ClaimReserveListing.xsd" />
	<xs:include schemaLocation="../xsd/ClaimPaymentListing.xsd" />
	<xs:include schemaLocation="../xsd/ClaimRecoveryListing.xsd" />

	<xs:annotation>
		<xs:documentation>This XML schema specifies ClaimImportComposite that
			encompases
			Claim information (includes Claim and Claim Policy), Claim
			Unit Information and other
			information required for Claim Import.
		</xs:documentation>
	</xs:annotation>

	<xs:complexType name="ClaimStatusDTO">
		<xs:annotation>
			<xs:documentation>Claim Status information</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="AuditWithExpireImportDTO">
				<xs:sequence>
					<xs:element name="claimId" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="openedDate" type="dateType" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim opened date.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="closedDate" type="dateType" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Closed date.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reOpenedDate" type="dateType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reopened date.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reClosedDate" type="dateType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reclosed date.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="actionCode" type="string" minOccurs="1"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim Status action code, 
								Maps to	CLM_STS_ACTN_CD Column from CLAIM_STATUS_ACTION_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="statusCode" type="string" minOccurs="1"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim Status code.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="stateCode" type="string" minOccurs="1"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim State code, 
								Maps to CLM_STT_CD Column from CLAIM_STATE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reasonCode" type="string" minOccurs="1"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim Status action reason code, 
								Maps to	CLM_ACTN_RSN_CD Column from CLAIM_ACTION_REASON_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="comments" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Comment for the Claim Status.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reasonName" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reason name, Maps to CLM_ACTN_RSN_NM Column
								from CLAIM_ACTION_REASON_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lastUpdatedUser" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Last updated user id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lastUpdatedDateTime" type="dateType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Last updated time.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="nextStatusActionCode" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Next status action code, Maps to
								CLM_STS_ACTN_CD Column from CLAIM_STATUS_ACTION_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="additionalReasons" type="claimStatusAdditionalReasonsType"
						minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Collection of Claim Status reasons as
								ClaimStatusAdditionalReasonDTO
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="expiredStatus" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Not used for Claim Import, Place holder for
								Claim Details
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimExternalSourceId" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>External Source Id for Claim.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="statusReserveFlag" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Not used for Claim Import, Place holder for
								Claim Details
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="statusWorkItemFlowFlag" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Not used for Claim Import, Place holder for
								Claim Details
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="statusCloseReserveFlag" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Status Close Reserve Flag.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="ClaimDetailsResponseDTO">
		<xs:annotation>
			<xs:documentation>Claim details response</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="claimId" type="long" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Used for importing individual entities without
						requiring Claim Information
					</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="claimNumber" type="string" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Used for importing individual entities without
						requiring Claim Information
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="claimExternalSourceId" type="string"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Used for importing individual entities without
						requiring Claim Information
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="claimDTO" type="ClaimDTO" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Claim Information, Required for Claim Import FNOL
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="units" type="ClaimUnitListType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of Claim Unit information used for
						Claim Import EDI
					</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="claimServices" type="claimDamageServicesDTOType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of Services for Claim to be imported.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="relatedClaims" type="relatedClaimType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of Related Claims to be imported
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="authorities" type="authorityType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collections of Authorities associated with the
						Claim
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="witnesses" type="witnessType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collections of Witnesses associated with the
						Claim
					</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="claimMiscTransactions" type="miscTransactionType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collections of Claim Miscellaneous transactions
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="claimReservesWithCoverages" type="reserveType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of Claim Reserves</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="claimPayments" type="claimPaymentType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of Claim Payments</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="recoveries" type="recoveryType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of Recovery Payments
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="billDetails" type="ClaimBillDTO"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collections of Bills associated with the Claim
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="notesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of Notes associated with the Claim
						Not
						Required
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="claimDocs" type="documentType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of Documents to be imported.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="injuredWorkerParticipant" type="dummyType"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Not used for Claim Import, Place holder for Claim
						Details
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="adjusterList" type="adjusterType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Not used for Claim Import, Place holder for Claim
						Details
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financialTransactionListing" type="financialTransactionListingType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Not used for Claim Import, Place holder for Claim
						Details
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="claimReserveSummary" type="claimReserveSummaryType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Not used for Claim Import, Place holder for Claim
						Details
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="claimReserveListing" type="claimReserveListingType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Not used for Claim Import, Place holder for Claim
						Details
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="claimPaymentSummary" type="ViewPaymentSummaryDTO"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Not used for Claim Import, Place holder for Claim
						Details
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="claimPaymentListing" type="claimPaymentListingType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Not used for Claim Import, Place holder for Claim
						Details
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="claimRecoveryListings" type="claimRecoveryListingType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Not used for Claim Import, Place holder for Claim
						Details
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="claimPaymentSchedule" type="ViewClaimScheduleLisitingCompositeDTO"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Not used for Claim Import, Place holder for Claim
						Details
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="isActiveCompanyLobCode" type="boolean"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>boolean indicating whether Compnay LOB is active
						or not
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="claimImportCompositeDTO" type="ClaimImportCompositeDTO">
		<xs:annotation>
			<xs:documentation>Claim import composite.</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:complexType name="ClaimImportCompositeDTO">
		<xs:annotation>
			<xs:documentation>Composite Type to include all Claim Information
				like Claim, Claim Policy, Claim Units, Participants, Financial
				Transactions etc
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ClaimDetailsResponseDTO">
				<xs:sequence>
					<xs:element name="policy" type="PolicyImportDTO"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Policy Information, Used in Claim Import FNOL.
								If available will be used for creating Claim, If not available
								then Policy Mediation will be used to retrieve Policy
								Information
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="persons" type="personType" minOccurs="0"
						maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Collection of Person information to the
								imported
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="organizations" type="organizationType"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Collection of Organizations</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimAssignments" type="assignmentType"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Collections of Claim Assignments.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="workItems" type="workItemType"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Collections of Work Items associated with the
								Claim
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="bills" type="claimBillType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Collection of Claim Bill</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimCases" type="caseType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Collection of Legal/Case Information associated
								with the Claim
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimBlocksCompositeDTO" type="ClaimBlocksCompositeDTO"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Collection of Blocks associated with the Claim
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="companyId" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Company Associated with the Claim.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="closeRecovery" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Indicates if Recovery Reserve needs to be
								closed after payment.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="ClaimBlocksCompositeDTO">
		<xs:annotation>
			<xs:documentation>Claim Block Composite</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="DataTransferObject">
				<xs:sequence>
					<xs:element name="claimBlocks" type="claimBlockType"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Claim Block list</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="segmentationBlockInd" type="boolean"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Segmentation block indicator</xs:documentation>
						</xs:annotation>
					</xs:element>					
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="claimBlockType">
		<xs:annotation>
			<xs:documentation>Collection of Claim Blocks</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="claimBlocksDTO" type="ClaimBlocksDTO"
				minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Claim blocks.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="class" type="collectionType" use="optional" />
	</xs:complexType>

	<xs:complexType name="ClaimBlocksDTO">
		<xs:annotation>
			<xs:documentation>Block information for the Claim.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="AuditWithDeleteDTO">
				<xs:sequence>
					<xs:element name="blockType" type="string" minOccurs="1"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Block Type</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="blockEffDate" type="dateType"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Block Effective Date</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="blockEndDate" type="dateType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Block End Date</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="blockReason" type="string" minOccurs="1"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Block Reason</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="blockLevel" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Block Level</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimId" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim Id</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="ClaimUnitListType">
		<xs:annotation>
			<xs:documentation>Collection of claim units.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="claimUnitVehicleDTO" type="ClaimUnitVehicleDTO"
				minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Claim vehicle unit</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="claimUnitDTO" type="ClaimUnitDTO"
				minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Claim unit.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="class" type="collectionType" use="optional" />
	</xs:complexType>


	<xs:complexType name="ClaimPolicyDTO">
		<xs:annotation>
			<xs:documentation>Claim Policy information</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="AuditWithDeleteDTO">
				<xs:sequence>
					<xs:element name="claimId" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Not used for Claim Import, Place holder for
								Claim Details
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="companyLOBId" type="long" minOccurs="1"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Company Line Of Business, 
								Maps to CMPY_LOB_ID	Column from COMPANY_LINE_OF_BUSINESS_XREF Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="regOfcCmpyOrgUnitId" type="long"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Company organization unit id, 
								Maps to	CMPY_ORG_UNT_ID Column from COMPANY_ORGANIZATION_UNIT_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="undrwrtCmpyOrgUnitId" type="long"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Underwriting Company organization unit id, 
								Maps to CMPY_ORG_UNT_ID Column from COMPANY_ORGANIZATION_UNIT_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="planCode" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Policy Plan code, 
								Maps to PLN_CD Column from PLAN_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="jurisdictionId" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Policy State, 
								Maps to JUR_ID Column from JURISDICTION_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="recordSourceCode" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Policy source system code - 
								Maps to REC_SRC_CD Column from RECORD_SOURCE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="pullStatusCode" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Policy pull code whether it is a partial pull,
								complete pull or auto pull - 
								Maps to CLM_PLCY_PULL_STS_CD Column from CLAIM_POLICY_PULL_STATUS_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="statusCode" type="string" minOccurs="0"
