<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<xs:annotation>
		<xs:documentation>Common Policy Schema.</xs:documentation>
	</xs:annotation>

	<xs:complexType name="PolicyUnitDescDTO">
		<xs:annotation>
			<xs:documentation>Policy Unit Description</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="unitIdTxt" type="string" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Unit Identifier, For Ex: VIN Number for Auto
						policy.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unitDataTypeCd" type="string" minOccurs="1"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Unit Data Type Code, Maps to UNT_DATA_TYP_CD
						Column from UNIT_DATA_TYPE_CD Table
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unitTypeCd" type="string" minOccurs="1"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Unit Type Code, Maps to UNT_TYP_CD Column from
						UNIT_TYPE_CD Table
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unitDesc" type="string" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Unit Description.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>