<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<xs:annotation>
		<xs:documentation>Case schema.</xs:documentation>
	</xs:annotation>
	
	<xs:element name="caseIssueStatusDTO" type="CaseIssueStatusDTO" />

	<xs:element name="caseDTO" type="ClientCaseDTO">
		<xs:annotation>
			<xs:documentation>Case.</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:complexType name="ClientCaseDTO">
		<xs:annotation>
			<xs:documentation>Client Case</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CaseDTO">
				<xs:sequence>
					<xs:element name="clientCaseStatus" type="ClientCaseStatusCompositeDTO"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case status composite</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="clientCaseIssueColl" type="clientCaseIssueType"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Case issued list</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="CaseDTO">
		<xs:annotation>
			<xs:documentation>Case Details</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BaseToolDTO">
				<xs:sequence>
					<xs:element name="agreementTypeCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Agreement Type Code, 
								Maps to AGRE_TYP_CD column
								in AGREEMENT_TYPE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseSourceCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case Source Code, 
								Maps to CASE_SRC_CD Column from
								CASE_SOURCE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseTypeCode" type="string" minOccurs="1"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case Type Code, 
								Maps to CASE_TYP_CD Column from
								CASE_TYPE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="casePriorityCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case Priority Code, 
								Maps to CASE_PRTY_CD column
								in CASE_PRIORITY_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseResolutionCode" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case Resolution Code, 
								Maps to CASE_RSOL_CD column
								in CASE_RESOLUTION_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseNumber" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reference number assigned to identify the case.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseExternalNumber" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reference number assigned to a case by an
								external entity.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseName" type="string" minOccurs="1"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Name assigned to the Case.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseInitiateDate" type="dateType"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case initiation date.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseDueDate" type="dateType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case Due Date.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseReasonDescription" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reason summary for the case.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseCompleteDate" type="dateType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case completion date.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseOwnerName" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case Owner Name</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="caseCategoryCode" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>case Category Code.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseProfileStatements" type="ProfileStatementItemType"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Case Profile Statements</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseContext" type="CaseContextDTO"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case Context Type</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseStatus" type="CaseStatusCompositeDTO"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case status composite</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseIssueColl" type="CaseIssueType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case issue list</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="participationCompositeDTO" type="CaseParticipationCompositeDTO"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case Participants</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="actionPlan" type="CaseActionPlanDTO"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case Action.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="isCaseGeneratedFromSystemRules" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Syatem generated case flag</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="CaseIssueType">
		<xs:annotation>
			<xs:documentation>Collection of case issue type</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="caseIssueDTO" type="CaseIssueDTO"
				minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Case issues.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="class" type="collectionType" use="optional" />
	</xs:complexType>

	<xs:complexType name="clientCaseIssueType">
		<xs:annotation>
			<xs:documentation>Collection of case issue type</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="caseIssueDTO" type="ClientCaseIssueDTO"
				minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Client case issues.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="class" type="collectionType" use="optional" />
	</xs:complexType>


	<xs:complexType name="ClientCaseIssueDTO">
		<xs:annotation>
			<xs:documentation>Client case issue</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CaseIssueDTO">
				<xs:sequence>
					<xs:element name="clientCaseIssueStatusColl" type="ClientCaseIssueStatusCompositeDTO"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case issue status composite</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="CaseContextDTO">
		<xs:annotation>
			<xs:documentation>Case Context Information</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="AuditDTO">
				<xs:sequence>
					<xs:element name="contextSourceCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Source of the Case context, 
								Maps to	CASE_CNTX_SRC_CD Column from CASE_CONTEXT_SOURCE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="agreementTypeCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Agreement Type for the Case, 
								Maps to AGRE_TYP_CD
								Column from AGREEMENT_TYPE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="agreementId" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Internal Agreement ID when the Case Source is
								Internal
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="recordSourceCode" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Record Source for the Case, 
								Maps to REC_SRC_CD
								Column from RECORD_SOURCE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="contextNumber" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case Context Number</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="contextVersionNumber" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case context version number to the particular
								agreement type.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="contextEffectiveDate" type="dateType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case context version number to the particular
								agreement type.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lineofBusiness" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>LOB associated with the Case, 
								Maps to LOB_CD
								Column from LINE_OF_BUSINESS_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="companyId" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Company Associated with the Case.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="ClientCaseStatusCompositeDTO">
		<xs:annotation>
			<xs:documentation>Case Issue status information.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CaseStatusCompositeDTO">
				<xs:sequence>
					<xs:element name="caseStatusDTO" type="CaseStatusDTO"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Current status of the Case</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="expiredCaseStatusCollection" type="caseStatusType"
						minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Collection of Expired statuses for Case as
								CaseStatusDTO
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="CaseStatusCompositeDTO">
		<xs:annotation>
			<xs:documentation>Case status composite</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="currentStatus" type="CaseStatusDTO"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Current status</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="expiredStatusCollection" type="caseStatusType"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Expired status list</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="caseStatusReasons" type="MapItemType"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Case status reson map, Maps to CASE_STS_RSN_CD
						Column from CASE_STATUS_REASON_CD Table.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="caseActionStatusCodeMap" type="MapItemType"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Case action status code map.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="caseResolutions" type="caseCodeLookupType"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Case resolution codes, Maps to CASE_RSOL_CD
						Column from CASE_RESOLUTION_CD Table.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="caseActions" type="caseCodeLookupType"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Case action lookup, Maps to CASE_ACTN_CD Column
						from CASE_ACTION_CD Table.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nextStatusReasonCode" type="string"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Next case status reson code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="caseCompleteDate" type="dateType"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Case compeletion date</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="caseResolutionCode" type="string"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Case resoltion code, Maps to CASE_RSOL_CD Column
						from CASE_RESOLUTION_CD Table.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="caseCodeLookupType">
		<xs:annotation>
			<xs:documentation>Collection of case code lookup</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="codeLookupBO" type="CodeLookupType"
				minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Lookup codes.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="class" type="collectionType" use="optional" />
	</xs:complexType>

	<xs:complexType name="caseStatusType">
		<xs:annotation>
			<xs:documentation>Collection of case status</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="caseStatusDTO" type="CaseStatusDTO"
				minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Case statuses.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="class" type="collectionType" use="optional" />
	</xs:complexType>

	<xs:complexType name="CaseStatusDTO">
		<xs:annotation>
			<xs:documentation>Case Status information.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="AuditWithExpireDTO">
				<xs:sequence>
					<xs:element name="caseId" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseStatusCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case Status, 
								Maps to CASE_STS_CD Column from
								CASE_STATUS_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseStatusReasonCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case Status Reason, 
								Maps to CASE_STS_RSN_CD
								Column from CASE_STATUS_REASON_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseStatusComments" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case Status Comment</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="nextStatusActionCode" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Next status action code.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="CaseIssueDTO">
		<xs:annotation>
			<xs:documentation>Issues associated with the Case.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="AuditDTO">
				<xs:sequence>
					<xs:element name="caseIssueCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case Issue Code, 
								Maps to CASE_ISS_CD Column from
								CASE_ISSUE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseIssueSumDesc" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case Issue Summary description.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseIssuePrimaryIndicator" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Indicates the Primary Case Issue
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseIssueStatusColl" type="CaseIssueStatusCompositeDTO"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case issue status composite</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="CaseIssueStatusCompositeDTO">
		<xs:annotation>
			<xs:documentation>Case Issue status information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="currentStatus" type="CaseIssueStatusDTO"
				minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Current status of the case issued
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="expiredStatusCollection" type="caseIssueStatusType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of Expired statuses for Case Issue as
						CaseIssueStatusDTO
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ClientCaseIssueStatusCompositeDTO">
		<xs:annotation>
			<xs:documentation>Case Issue status information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="caseIssueStatus" type="CaseIssueStatusDTO"
				minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Current status of the Case Issued.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="expiredCaseIssueStatusCollection" type="caseIssueStatusType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of Expired statuses for Case Issue as
						CaseIssueStatusDTO
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="caseIssueStatusType">
		<xs:annotation>
			<xs:documentation>Collection of issued case status</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="caseIssueStatus" type="CaseIssueStatusDTO"
				minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Case issue statuses.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="class" type="collectionType" use="optional" />
	</xs:complexType>

	<xs:complexType name="CaseIssueStatusDTO">
		<xs:annotation>
			<xs:documentation>Status of the Case Issued.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="AuditWithExpireDTO">
				<xs:sequence>
					<xs:element name="issueStatusCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case Issued status code, 
								Maps to CASE_ISS_STS_CD
								Column from CASE_ISSUE_STATUS_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="resolutionAmount" type="dollarAmount"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Resolution Amount for the Case Issue status
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="resolutionTermsDesc" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Resolution term description for the Case Issue
								status
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="CaseParticipationCompositeDTO">
		<xs:annotation>
			<xs:documentation>Case Participation information.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="DataTransferObject">
				<xs:sequence>
					<xs:element name="caseId" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="caseParticipations" type="CaseParticipationDTOType"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Collection of Case Participants as
								CaseParticipationDTO
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="CaseParticipationDTOType">
		<xs:annotation>
			<xs:documentation>Collection of case participation</xs:documentation>
		</xs:annotation>
		<xs:sequence>
