<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<xs:annotation>
		<xs:documentation>Claim Reserve Schema.</xs:documentation>
	</xs:annotation>
	
	<xs:element name="authClaimPaymentStatusDTO" type="AuthorizationClaimPaymentStatusDTO" />

	<xs:element name="claimReserve" type="CALClaimReserveDTO">
		<xs:annotation>
			<xs:documentation>Claim reserve.</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:complexType name="CALClaimReserveDTO">
		<xs:annotation>
			<xs:documentation>Claim Reserves.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="AuditWithImportDTO">
				<xs:sequence>
					<xs:element name="calClaimIdDerived" type="long"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reserve claim id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="calClaimUnitIdDerived" type="long"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reserve claim unit id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="calClaimPtcpId" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reserve claim participant id.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="clmCoverageCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim coverage code, Maps to CLM_COV_CD Column
								from CLAIM_COVERAGE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="clmReserveTypeCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim reserve type code, Maps to
								CLM_RSRV_TYP_CD Column from CLAIM_RESERVE_TYPE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="clmLossTypeCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim loss type code, Maps to CLM_LOSS_TYP_CD
								Column from CLAIM_LOSS_TYPE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="calClaimReserveAmtDerived" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim reserve amount.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="monetaryCategoryCode" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Rserve monetary category code, Maps to
								CLM_MNY_CTG_CD Column from CLAIM_MONETARY_CATEGORY_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="preApprovedIndicator" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Approved reserve indicator.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="averageReserveIndicator" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Averaged reserve indicator.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="calClaimReserveStatus" type="CalClaimReserveStatusCompositeDTO"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim reserve status.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="calClaimReserveTransactionCollections"
						type="claimReserveTransactionCollectionType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Claim reserve transaction list
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reasonCode" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reserve transaction reason code, Maps to
								CLM_RSRV_TRAN_RSN_NC_CD Column from CLAIM_RESERVE_TRAN_RSN_NC_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="actionType" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reserve Tran action, Maps to
								CLM_RSRV_TRAN_TYP_NC_CD Column from CLAIM_RESERVE_TRAN_TYP_NC_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="companyLobId" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Company LOB Id, Maps to CMPY_LOB_ID Column from
								COMPANY_LINE_OF_BUSINESS_XREF Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="existingOutstandingAmount" type="dollarAmount"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Outstanding amount.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="adjustmentIndicator" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Adjustment flag.
								incr - Increase amount
								decr -
								Decrease amount
								repl - Replace amount
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="adjustmentAmount" type="dollarAmount"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Adjustment amount.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimCoverageId" type="long"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim coverage id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="unconfirmedCoverage" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Unconfirmed coverage indicator.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reserveTxnAuthStatus" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reserve auth status.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="userIdAuthorizer" type="long"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Auth user id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="companyOrganizationUnitIdAuthorizer"
						type="long" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Authorizer org unit id, Maps to CMPY_ORG_UNT_ID
								Column from COMPANY_ORGANIZATION_UNIT_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="companyOrgUnitIdOrginator" type="long"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Origin org unit id, Maps to CMPY_ORG_UNT_ID
								Column from COMPANY_ORGANIZATION_UNIT_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="userIdOrginator" type="long"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Orgin user id</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="selectedAuthGroup" type="long"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>place holder for the Authorizable Request
								interface
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="authGroupProvided" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Authorization group.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="paymentTypeCode" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Payment type code, Maps to CLM_MNY_TYP_CD
								Column from CLAIM_MONETARY_TYPE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="paymentAdjustmentAmount" type="dollarAmount"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Payment addjustment amount.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimNumber" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim number.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimPolicyUnitId" type="long"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim policy unit id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="importReserve" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Import reserve flag.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="teamId" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Team id, Maps to CMPY_ORG_UNT_ID Column from
								COMPANY_ORGANIZATION_UNIT_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="teamName" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Team name.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="user" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reserve user</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="transactionAmount" type="dollarAmount"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reserve transaction amount</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reserveTransactionDate" type="dateType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reserve transaction date</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="externalSourceId" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>External source identifier.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="unitDescription" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim unit description</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="participantName" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim reserve participant name.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reserveTypeName" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reserve type name, Maps to CLM_RSRV_TYP_NM
								Column from CLAIM_RESERVE_TYPE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="payment" type="boolean" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Payment flag.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="recoveryAdjustmentAmount" type="dollarAmount"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Recovery adjument amount.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="adjustedOutsandingReserve" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Adjusted outstanding reserve.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="externalParticipantIdentifier" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim participant external id.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="externalUnitIdentifier" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim unit external id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="externalClaimNumber" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>External claim number.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="externalClaimIdentifier" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim external id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="authDetails" type="dummyType"
						minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Not used for Claim Import, Place holder for
								Claim Details
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="coverages" type="claimCoverageType"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Claim coverage type.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="paymentAuthorizations" type="paymentAuthorizationsType"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Payment authorization.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reopenAndCloseReserve" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reopen reserve flag</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="agreementDataMap" type="MapItemType"
						minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Agreement Data Type Map.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="versionGroupId" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Version group id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="unitIds" type="longCollectionType"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Collection of Unit Ids, Not used for Claim
								Import, Place holder for Claim Details
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reserveLevel" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Not used for Claim Import, Place holder for
								Claim Details
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="participantTypeName" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Used for showing error message when participant
								is expired
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="declineFastTrackPayment" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Not used for Claim Import, Place holder for
								Claim Details
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="closeReserveIssuePayment" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Not used for Claim Import, Place holder for
								Claim Details
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="userIdRqst" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Not used for Claim Import, User who created a
								financial transaction which required authorization
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="importPaymentFlow" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Indicates that the payment is through import.
								This will be set in the integration code.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<!-- Newly Added for 8.1 Release -->
					<xs:element name="classCode" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reserve class code, Maps to CLM_CLS_CD_CD
								Column from CLAIM_CLASS_CODE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="classSuffixCode" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Class suffix code, Maps to CLM_CLS_CD_SUFX_CD
								Column from CLAIM_CLASS_CODE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="closedReserveFromFastTrack" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Fast track close reserve indicator.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<!-- End -->
					<xs:element name="operationType" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Operation type.
								rsrv - Rserve
								fasttrkpmt - Fast
								track payment
								authpmt - Authorized payment
								authrsrv - Authorized
								reserve
								mdfypendpmt - Modify pending payment
								adjstrcvry - Adjust
								recovery
								fasttrkrcvry - Fast track recovery
								mdfypmt - Modify
								payment
								rcvry - Post recovery
								adjstpmt - Adjust payment
								issuepmtaftrcls - Issue payment after close
								issuepmt - Issue
								paument
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="benefitCalculatedWorksheetId" type="long"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Benefit calculator work sheet id, (Workers
								comp)
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="authorizingGroup" type="long"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Authorization group id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="authorizationGroupProvided" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Authorization group flag.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="authorizationDetails" type="dummyType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Internal use.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="authorizingUserId" type="long"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Authorization user id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="multipleAuthorization" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Multiple auth required flag.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="additionalAuthorizer" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Additional authorizer flag.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="signatureCount" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Signature count.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="requiredSignatureCount" type="long"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Mandatory Signature count.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="benefitWorksheetDTO" type="claimIndemnityBenefitWorksheetDTO"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim Indemnity Benefit Worksheet.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="claimIndemnityBenefitWorksheetDTO">
		<xs:annotation>
			<xs:documentation>Indemnity Benefit.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="AuditDTO">
				<xs:sequence>
					<xs:element name="claimNumber" type="string" minOccurs="1"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim Number.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="participantId" type="long" minOccurs="1"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Participant id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="partyId" type="long" minOccurs="1"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Party id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="jurId" type="long" minOccurs="1"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Jurisdiction id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimId" type="long" minOccurs="1"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="effectiveDateSourceType" type="dateType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Effective date.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reserveTypeCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reserve Type code, Maps to CLM_RSRV_TYP_CD
								Column from CLAIM_RESERVE_TYPE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lossTypeCode" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Loss Type code, 
								Maps to CLM_LOSS_TYP_CD Column from CLAIM_LOSS_TYPE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimWageMethodCode" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim Wage method code, Maps to CLM_WG_METH_CD
								Column from	CLAIM_WAGE_METHOD_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="jurisdiction" type="string" minOccurs="0"
