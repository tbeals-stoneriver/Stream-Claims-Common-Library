			<xs:extension base="AuditWithDeleteDTO">
				<xs:sequence>
					<xs:element name="claimId" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Existing claim that the claim relationship is
								being added to
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="relatedClaimId" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Represents the claim that is being related to
								an existing specific claim.
								If this is an external claim, this
								will be null and the user will
								enter the External Claim Number
								and Name.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="relationshipTypeCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Type of relationship being established between
								the Claims, 
								Maps to CLM_RLT_TYP_CD Column from CLAIM_RELATIONSHIP_TYPE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="relationshipTypeComment" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>User entered description of the nature of the
								relationship between the
								Claims when the selected relationship
								type is Other.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="externalClaimFlag" type="boolean"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Represents the source of the Related Claim as
								either being a
								Claim internal to the system (Internal indication)
								or external to the
								system (External Indication).
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="externalClaimNumber" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Represents the user entered reference number of
								the Claim to be related to
								the existing specified Claim, when the
								related claim is identified
								as External.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="externalClaimInsuredName" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Represents the Claim Name associated to the
								existing specified Claim for a
								related Claim identified as
								External.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="createdBy" type="string" minOccurs="0"
