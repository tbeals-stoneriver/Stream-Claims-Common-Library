<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<xs:annotation>
		<xs:documentation>Claim common schema.</xs:documentation>
	</xs:annotation>

	<xs:complexType name="ClaimDTO">
		<xs:annotation>
			<xs:documentation>Common Claim</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="AuditWithDeleteDTO">
				<xs:sequence>
					<xs:element name="claimTypeCode" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim Type, 
								Maps to CLM_TYP_CD Column from CLAIM_TYPE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="catId" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Catastrophe id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="catIdMCO" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Cat MCO id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="jurisdictionId" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>State for the Claim, 
								Maps to JUR_ID Column from JURISDICTION_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lossIndicator" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Loss indicator flag.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="weather" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Weather condition when the loss occurred, 
								Maps to WTHR_COND_CD Column from WEATHER_CONDITION_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="notificationSourceCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Notification Source for the Claim, 
								Maps to	NTF_SRC_CD Column from NOTIFICATION_SOURCE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="notificationMethodCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Notification method for the Claim, 
								Maps to NTF_METH_CD Column from NOTIFICATION_METHOD_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lossDateTimeZoneCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim Loss Date Time zone, 
								Maps to TIME_ZN_CD Column from TIME_ZONE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimMadeDateTimeZoneCode" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim Made Date Time zone, 
								Maps to TIME_ZN_CD Column from TIME_ZONE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="notificationDateTimeZoneCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim Notification Date Time zone, 
								Maps to	TIME_ZN_CD Column from TIME_ZONE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="totRecDeductAmt" type="long"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Total recovery deductible amount
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimNumber" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim number.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lossLocationDescription" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Loss location description.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lossDescription" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Loss description.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="callerName" type="string" minOccurs="1"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Caller name.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="externalID" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>External id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="dateOfLossDate" type="dateType"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Date of loss.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimMadeDate" type="dateType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim made date.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="notificationDate" type="dateType"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Notification date.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lossDateTime" type="dateType"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Loss time.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimMadeTime" type="dateType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim made time.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="notificationTime" type="dateType"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Notification time.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lossPriorityCode" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Priority of the Loss, 
								Maps to LOSS_PRTY_CD column	in LOSS_PRIORITY_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lossComplexityCode" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Complexity of the Loss, 
								Maps to LOSS_CMPLX_CD Column from LOSS_COMPLEXITY_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="catManualFlag" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Flag indicating if a catastrophe was assigned
								to this claim manually (Y) or by the system (N)
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimSourceCode" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Originating source of the Claim, such as Quick
								Claim, FNOL, WEB, Import - 
								Maps to CLM_SRC_CD Column from CLAIM_SOURCE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="companyLOBId" type="long" minOccurs="1"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Company Line Of Business, 
								Maps to CMPY_LOB_ID	Column from COMPANY_LINE_OF_BUSINESS_XREF Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lobCode" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>LOB of the Claim, Maps to LOB_CD Column from
								LINE_OF_BUSINESS_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="unverifiedPolicy" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Indicates whether the policy used for the claim
								is unverified or not
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="occurrenceDateVerifyIndicator" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Indicates whether Date of Occurrence is
								estimated and needs to be verified with certainty.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimPolicyNumber" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim policy number.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="causeOfLossCode" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Cause of loss.</xs:documentation>
						</xs:annotation>
					</xs:element>					
					<xs:element name="clmParticipants" type="claimParticipantsType"
						minOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim participants.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="clmAddress" type="ClaimAddressDTO"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim address.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="policy" type="ClaimPolicyDTO"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim policy.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimStatus" type="ClaimStatusCompositeDTO"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim status.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimIndicators" type="ClaimIndicatorDTO"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim indictors.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="coverages" type="dummyType" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Not used for Claim Import, Place holder for
								Claim Details
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="contactInfo" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Not used for Claim Import</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="calClaimReportDate" type="claimReportDateType"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Claim report date.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimAliasNumbers" type="dummyType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Not used for Claim Import, Place holder for
								Claim Details
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="catastrophesExcludedFrom" type="dummyType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Not used for Claim Import, Place holder for
								Claim Details
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimPolicyRiskLocationDTO" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Not used for Claim Import, Place holder for
								Claim Details
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="companyLobEndDt" type="dateType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Not used for Claim Import, Place holder for
								Claim Details
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="reinsuranceRetentionDetail" type="ClaimReinsuranceRetentionDTOType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reinsurance retention detail.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="externalSourceId" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>External Source Id for Claim.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="wcLiteUnitLocationId" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Workers comp unit location id.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimJurisdictionProfiles" type="ProfileStatementItemType"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Jurisdiction Questions and answers associated
								to the Claim
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimLossProfiles" type="ProfileStatementItemType"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Loss Questions and answers associated to the
								Claim
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimLossIndicatorProfiles" type="ProfileStatementItemType"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Loss Indicator Questions and answers associated
								to the Claim
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimPolicyIndicatorProfiles" type="ProfileStatementItemType"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Policy Indicator Questions and answers
								associated to the Claim
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimReserveSummary" type="claimReserveSummaryType"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Claim reserve summary.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimQuestions" type="ProfileStatementItemType"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Questions and answers associated to the Claim
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimUnitDTO" type="ClaimUnitDTO"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>Claim unit type.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="ClaimReinsuranceRetentionDTOType">
		<xs:sequence>
			<xs:element name="claimReinsuranceRetentionDTO" type="ClaimReinsuranceRetentionDTO"
				minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Reinsurance retentions.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="class" type="collectionType" use="optional" />
	</xs:complexType>

	<xs:complexType name="ClaimReinsuranceRetentionDTO">
		<xs:annotation>
			<xs:documentation>Reinsurance Retention details</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="AuditDTO">
				<xs:sequence>
					<xs:element name="claimId" type="long" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim id.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reinsuranceTypeCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reinsurance type code, Maps to CLM_REINS_TYP_CD
								Column from CLAIM_REINSURANCE_TYPE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reinsuranceRetention" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reinsurance retention, Maps to CLM_REINS_TYP_CD
								Column from CLAIM_REINSURANCE_TYPE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="prevReinsuranceRetention" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Previous Reinsurance retention flag.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reinsuranceOverride" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reinsurance override.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="limitReachedEventDispatchInd" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Limit reached event indicator.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="overrideComment" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Comment override.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="claimReserveSummaryType">
		<xs:sequence>
			<xs:element name="viewClaimReserveTransClaimLevelResult"
				type="ViewClaimReserveTransClaimLevelResult" minOccurs="0"
				maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>>Claim transaction level reserves.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="viewClaimReserveTransUnitLevelResult"
				type="ViewClaimReserveTransUnitLevelResult" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Claim unit transaction level reserves.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="viewClaimReserveTransPartLevelResult"
				type="ViewClaimReserveTransPartLevelResult" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Claim participant transaction level reserves.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="viewClaimReserveTransCovLevelResult"
				type="ViewClaimReserveTransCovLevelResult" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Claim coverage transaction level reserves.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="viewClaimReserveTransReserveLevelResult"
				type="ViewClaimReserveTransReserveLevelResult" minOccurs="0"
				maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Claim reserve transaction level reserves.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="class" type="collectionType" use="optional" />
	</xs:complexType>

	<xs:complexType name="claimParticipantsType">
		<xs:sequence>
			<xs:element name="claimParticipantsDTO" type="CALClaimParticipationDTO"
				minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Claim participants.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="class" type="collectionType" use="optional" />
	</xs:complexType>

	<xs:complexType name="claimReportDateType">
		<xs:sequence>
			<xs:element name="claimReportDateDTO" type="ClaimReportDateDTO"
				minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Claim report date.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="class" type="collectionType" use="optional" />
	</xs:complexType>

	<xs:complexType name="ClaimReportDateDTO">
		<xs:annotation>
			<xs:documentation>Report Date information for the Claim.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="AuditWithDeleteDTO">
				<xs:sequence>
					<xs:element name="claimNotificationSourceCode" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Not used for Claim Import, Place holder for
								Claim Details
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimReportTypeCode" type="string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim Report Type, 
								Maps to CLM_RPT_DT_TYP_CD Column from CLAIM_REPORT_DATE_TYPE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimReportDate" type="dateType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim Report Date</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimReportTime" type="dateType"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim Report Time</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimReportTimeZone" type="string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Claim Report Time Zone, 
								Maps to TIME_ZN_CD Column from TIME_ZONE_CD Table.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="requiredForRelease" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Not used for Claim Import, Place holder for
								Claim Details
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="ClaimIndicatorDTO">
		<xs:annotation>
			<xs:documentation>Loss Indicators for Claim.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="AuditWithExpireDTO">
				<xs:sequence>
					<xs:element name="catastropheIndicator" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Indicates whether the claim is associated to
								any Catastrophe
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="claimReviewIndicator" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Indicates whether the claim is being reviewed
								by an auditor or any other person.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="externalIndicator" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Indicates whether the Claim is from an external
								system
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="largeLossIndicator" type="boolean"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Indicates whether the claim is a large loss
								claim.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
